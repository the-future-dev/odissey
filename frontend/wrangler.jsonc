{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "odissey-frontend",
  "main": "./worker/index.ts",
  "compatibility_date": "2025-06-07",
  "compatibility_flags": [
    "global_fetch_strictly_public"
  ],
  "assets": {
    "directory": "./dist",
    "not_found_handling": "single-page-application",
    "binding": "ASSETS"
  },
  "observability": {
    "enabled": true
  },
  "vars": {}
} 